{
  "buildCommand": "cd frontend && npm run build",
  "installCommand": "cd frontend && npm install",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "ignores": ["README.md"],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://tradesmate-production.up.railway.app/api/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://tradesmate-production.up.railway.app"
  }
}
